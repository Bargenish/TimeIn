@model IEnumerable<Time_In.Models.Service>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.serviceName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.businessID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.serviceLength)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.serviceName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.businessID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.serviceLength)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
